docker run --name jira -e POSTGRES_USER=jiradb -e POSTGRES_PASSWORD=jiradb -e POSTGRES_DB=jiradb -e DB_HOST=abc.xyz.com -e DB_PORT=5432 -p 8080:8080 -v jirahome:/opt/jira-home -it dvnagesh/jira-centos:7.2.4 bash

